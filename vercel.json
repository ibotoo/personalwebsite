{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"version": 2,
	"alias": "ibrahimsancar.com",
	"cleanUrls": true,
	"public": true,
	"regions": [
		"lhr1"
	],
	"redirects": [
		{
			"source": "/portfolio",
			"destination": "/"
		},
		{
			"source": "/blog",
			"destination": "/"
		}
	],
	"buildCommand": "pnpm install --no-frozen-lockfile && pnpm run build",
	"outputDirectory": ".next",
	"framework": "nextjs",
	"installCommand": "pnpm install --no-frozen-lockfile"
}